<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:storeCard="clr-namespace:StoreCard"
    xmlns:converters="clr-namespace:StoreCard.Converters">
    <converters:NullVisibilityConverter
        x:Key="NullVisibilityConverter" />
    <BooleanToVisibilityConverter
        x:Key="BooleanToVisibilityConverter" />
    <converters:InverseBooleanToVisibilityConverter
        x:Key="InverseBooleanToVisibilityConverter" />
    <converters:NotBlankVisibilityConverter
        x:Key="NotBlankVisibilityConverter" />
</ResourceDictionary>